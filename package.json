{
  "private": true,
  "name": "bali",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "per-env",
    "start:production": "npm run -s serve",
    "start:development": "npm run -s dev",
    "build": "preact build --template src/template.html --no-prerender --no-esm --sw false && cp _headers build/_headers && cp _redirects build/_redirects && cp sw.js build/sw.js",
    "serve": "preact build  --template src/template.html --no-prerender --no-esm --sw false && preact serve",
    "dev": "preact watch  --template src/template.html --no-esm --sw false",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-synacor": "^3.0.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "per-env": "^1.0.2",
    "preact-cli": "^3.0.0-rc.6",
    "preact-render-spy": "^1.2.1",
    "prettier": "1.19.1"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "blockstack": "^19.3.0",
    "core-js": "^3.6.1",
    "dayjs": "^1.8.18",
    "leaflet": "^1.6.0",
    "preact": "^10.1.1",
    "preact-router": "^3.1.0",
    "regenerator-runtime": "^0.13.3",
    "typeface-roboto": "^0.0.75",
    "typeface-roboto-condensed": "^0.0.75",
    "uuid": "^3.3.2"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js"
    ],
    "testRegex": "(/(__tests__|tests)/.*|(\\.|/)(test|spec))\\.jsx?$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/tests/__mocks__/*"
    ],
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "^./style$": "identity-obj-proxy",
      "^preact$": "<rootDir>/node_modules/preact/dist/preact.min.js",
      "^react$": "preact/compat",
      "^react-dom$": "preact/compat",
      "^create-react-class$": "preact/compat/lib/create-react-class",
      "^react-addons-css-transition-group$": "preact-css-transition-group"
    }
  }
}
